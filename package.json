{
  "name": "jerry-chong-portfolio-website",
  "version": "2.0.0",
  "main": "index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jerrychong25/jerrychong25.github.io.git"
  },
  "homepage": "https://jerrychong25.github.io/",
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "coverageDirectory": "./coverage/",
    "testURL": "http://localhost:8080",
    "transform": {
      "\\.(js|jsx)$": "./transformPreprocessor.js"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy",
      "^preact$": "<rootDir>/node_modules/preact/dist/preact.min.js",
      "^react$": "preact-compat",
      "^react-dom$": "preact-compat",
      "^create-react-class$": "preact-compat/lib/create-react-class",
      "^react-addons-css-transition-group$": "preact-css-transition-group"
    }
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build --template template.html",
    "build-test": "npm run tests-only && preact build --template template.html",
    "serve": "preact build && preact serve",
    "dev": "preact watch --template template.html",
    "lint": "eslint src --fix",
    "tests-only": "jest --no-cache",
    "test": "npm run lint && jest --no-cache",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^4.5.0",
    "eslint-config-synacor": "^2.0.0",
    "handlebars": "^4.7.6",
    "if-env": "^1.0.0",
    "jest": "^21.2.1",
    "preact-cli": "^2.2.1",
    "preact-render-spy": "^1.1.0"
  },
  "dependencies": {
    "ajv": "^5.5.2",
    "font-awesome": "^4.7.0",
    "foundation-sites": "^6.6.2",
    "jquery": "^3.5.0",
    "node-sass": "^4.13.1",
    "postcss": "^6.0.23",
    "preact": "^8.5.3",
    "preact-compat": "^3.17.0",
    "preact-router": "^2.5.7",
    "react-ga": "^2.7.0",
    "sass-loader": "^6.0.6",
    "what-input": "^5.2.7",
    "snyk": "^1.316.1"
  },
  "snyk": true
}
